<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="${Vendorname}_${Modulename}::${modulename}" title="${Modulename}">
                        <resource id="${Vendorname}_${Modulename}::${entityname}" title="${Entityname}"/>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
